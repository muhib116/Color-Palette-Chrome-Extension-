{
    "manifest_version": 3,
    "name": "Image to Color Palette",
    "description": "",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/icon128.png"
    },
    "permissions": ["scripting", "activeTab", "storage"],
    "icons": {
        "16":"icons/icon16.png",
        "48":"icons/icon48.png",
        "128":"icons/icon128.png"
    },
    "background":{
        "service_worker":"background.js",
        "type": "module"
    }
}